{
	"name": "MZQC Python Development container",
	"dockerFile": "Dockerfile",
	"context": "..",
	"extensions": [
		"ms-python.python", "littlefoxteam.vscode-python-test-adapter"

	],
	"settings": {
		"python.pythonPath": "/usr/local/bin/python"
	},
	"postCreateCommand": "sudo pip install -e .",
	"runArgs": ["-u", "vscode"]
}
